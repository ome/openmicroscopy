  <info
      organisation="omero"
      module="omero_client"
      status="integration"/>
  <configurations>
    <conf name="build"/>
    <conf name="client"/>
  </configurations>
  <publications>
    <artifact name="omero_client" type="jar"/>
    <artifact name="omero_client" type="pom"/>
    <artifact name="omero_client" type="source" ext="jar" m:classifier="sources"/>
    <artifact name="omero_client" type="javadoc" ext="jar" m:classifier="javadoc"/>
  </publications>
  <dependencies>
    <!-- Internal -->
    <dependency org="omero" name="blitz" rev="${omero.version}" changing="true" conf="build->build"/>
    <!-- Bio-Formats -->
    <dependency org="ome" name="formats-bsd" rev="${versions.bioformats}" transitive="true">
        <exclude org="org.perf4j"/>
        <exclude org="org.slf4j"/>
        <exclude org="xml-apis"/>
        <exclude org="xml-apis"/>
    </dependency>
    <dependency org="ome" name="formats-gpl" rev="${versions.bioformats}" transitive="true">
        <exclude org="org.slf4j"/>
    </dependency>
    <dependency org="org.glassfish" name="javax.json" rev="1.0.4" force="true" transitive="true">
    	<artifact name="javax.json" type="jar" url="http://central.maven.org/maven2/org/glassfish/javax.json/1.0.4/javax.json-1.0.4.jar"/>
    </dependency>
    <dependency org="org.apache.httpcomponents" name="httpclient" rev="4.5.3" force="true" transitive="true">
    	<artifact name="httpclient" type="jar" url="http://central.maven.org/maven2/org/apache/httpcomponents/httpclient/4.5.3/httpclient-4.5.3.jar"/>
    </dependency>
    <dependency org="org.apache.httpcomponents" name="httpcore" rev="4.4.6" force="true" transitive="true">
    	<artifact name="httpcore" type="jar" url="http://central.maven.org/maven2/org/apache/httpcomponents/httpcore/4.4.6/httpcore-4.4.6.jar"/>
    </dependency>
    <dependency org="org.apache.httpcomponents" name="httpmime" rev="4.5.3" force="true" transitive="true">
    	<artifact name="httpmime" type="jar" url="http://central.maven.org/maven2/org/apache/httpcomponents/httpmime/4.5.3/httpmime-4.5.3.jar"/>
    </dependency>
  </dependencies>
</ivy-module>


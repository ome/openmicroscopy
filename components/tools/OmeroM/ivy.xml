<ivy-module version="1.0">
  <info
      organisation="omero"
      module="OmeroMatlab"
      status="integration"/>
  <configurations>
    <conf name="build"/>
    <conf name="client"/>
  </configurations>
  <publications>
    <artifact name="OmeroMatlab" type="zip"/>
  </publications>
  <dependencies defaultconfmapping="build,client->default">
    <dependency org="omero" name="omero_client" rev="${omero.version}" changing="true" conf="build->build;client->client" transitive="false"/>
    <dependency org="omero" name="OmeroImporter" rev="${omero.version}" conf="build,client->build"/>
  </dependencies>
</ivy-module>

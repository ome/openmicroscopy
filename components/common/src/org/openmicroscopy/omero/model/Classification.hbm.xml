<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="org.openmicroscopy.omero.model.Classification" 
    table="classification"
>

    <id
        name="attributeId"
        type="java.lang.Integer"
        column="attribute_id"
    >
        <generator class="assigned" />
    </id>

    <property
        name="confidence"
        type="java.lang.Float"
        column="confidence"
        length="4"
    />
    <property
        name="valid"
        type="java.lang.Boolean"
        column="valid"
        length="1"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Image -->
    <many-to-one
        name="image"
        class="org.openmicroscopy.omero.model.Image"
        not-null="true"
    >
        <column name="image_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Category -->
    <many-to-one
        name="category"
        class="org.openmicroscopy.omero.model.Category"
        not-null="true"
    >
        <column name="category" />
    </many-to-one>
    <!-- bi-directional many-to-one association to ModuleExecution -->
    <many-to-one
        name="moduleExecution"
        class="org.openmicroscopy.omero.model.ModuleExecution"
        not-null="true"
    >
        <column name="module_execution_id" />
    </many-to-one>

</class>
</hibernate-mapping>

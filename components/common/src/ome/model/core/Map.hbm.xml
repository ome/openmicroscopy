<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="ome.model.core.Map" 
    table="map"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="map"
    </meta>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="id"
        type="java.lang.Integer"
        column="id"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="sequence"
            type="java.lang.Integer"
            column="id"

           @hibernate.generator-param
	        name="sequence"
	        value="map_seq"
        </meta>
        <generator class="sequence">
            <param name="sequence">map_seq</param>
        </generator>
    </id>


	
<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
	 COMMON
     ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <version
        name="version"
        type="java.lang.Integer"
        column="version"
        unsaved-value="negative"
    >
        <meta attribute="field-description">
           @hibernate.version
            column="version"
            unsaved-value="negative"
        </meta>
    </version>

    <component name="permissions"
		class="ome.model.internal.Permissions">
		<property name="bytes" type="binary" column="permissions"/>
    </component>
    <many-to-one name="owner" class="ome.model.meta.Experimenter" not-null="true" cascade="save-update"/>
    <many-to-one name="creationEvent" class="ome.model.meta.Event" not-null="true" cascade="save-update"/>		
    <many-to-one name="updateEvent" class="ome.model.meta.Event" not-null="false"/>	
	
<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
	 SPECIFIC
     ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	
	
    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Pixel -->
    <many-to-one
        name="pixel"
        class="ome.model.core.Pixel"
        not-null="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="pixel"         
        </meta>
        <column name="pixel" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Roi5D -->
    <many-to-one
        name="roi5d"
        class="ome.model.core.Roi5D"
        not-null="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="roi5d"         
        </meta>
        <column name="roi5d" />
    </many-to-one>
    <!-- bi-directional many-to-one association to RoiSet -->
    <many-to-one
        name="roiSet"
        class="ome.model.core.RoiSet"
        not-null="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="set"         
        </meta>
        <column name="set" />
    </many-to-one>

</class>
</hibernate-mapping>

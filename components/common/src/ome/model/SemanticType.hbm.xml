<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="ome.model.SemanticType" 
    table="semantic_types"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="semanticTypeId"
        type="java.lang.Integer"
        column="semantic_type_id"
    >
        <generator class="sequence">
            <param name="sequence">this_seq_does_not_exist</param>
        </generator>
    </id>

    <property
        name="granularity"
        type="java.lang.String"
        column="granularity"
        not-null="true"
        length="1"
    />
    <property
        name="name"
        type="java.lang.String"
        column="name"
        not-null="true"
        unique="true"
        length="64"
    />
    <property
        name="description"
        type="java.lang.String"
        column="description"
        length="-1"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to SemanticTypeOutput -->
    <set
        name="semanticTypeOutputs"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="semantic_type_id" />
        </key>
        <one-to-many 
            class="ome.model.SemanticTypeOutput"
        />
    </set>
    <!-- bi-directional one-to-many association to FormalOutput -->
    <set
        name="formalOutputs"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="semantic_type_id" />
        </key>
        <one-to-many 
            class="ome.model.FormalOutput"
        />
    </set>
    <!-- bi-directional one-to-many association to FormalInput -->
    <set
        name="formalInputs"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="semantic_type_id" />
        </key>
        <one-to-many 
            class="ome.model.FormalInput"
        />
    </set>
    <!-- bi-directional one-to-many association to SemanticElement -->
    <set
        name="semanticElements"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="semantic_type_id" />
        </key>
        <one-to-many 
            class="ome.model.SemanticElement"
        />
    </set>

</class>
</hibernate-mapping>

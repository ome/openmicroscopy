<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="ome.model.AnalysisChainLink" 
    table="analysis_chain_links"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="analysisChainLinkId"
        type="java.lang.Integer"
        column="analysis_chain_link_id"
    >
        <generator class="sequence">
            <param name="sequence">this_seq_does_not_exist</param>
        </generator>
    </id>


    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to AnalysisChain -->
    <many-to-one
        name="analysisChain"
        class="ome.model.AnalysisChain"
        not-null="true"
    >
        <column name="analysis_chain_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to AnalysisChainNode -->
    <many-to-one
        name="analysisChainNodeByToNode"
        class="ome.model.AnalysisChainNode"
        not-null="true"
    >
        <column name="to_node" />
    </many-to-one>
    <!-- bi-directional many-to-one association to AnalysisChainNode -->
    <many-to-one
        name="analysisChainNodeByFromNode"
        class="ome.model.AnalysisChainNode"
        not-null="true"
    >
        <column name="from_node" />
    </many-to-one>
    <!-- bi-directional many-to-one association to FormalOutput -->
    <many-to-one
        name="formalOutput"
        class="ome.model.FormalOutput"
        not-null="true"
    >
        <column name="from_output" />
    </many-to-one>
    <!-- bi-directional many-to-one association to FormalInput -->
    <many-to-one
        name="formalInput"
        class="ome.model.FormalInput"
        not-null="true"
    >
        <column name="to_input" />
    </many-to-one>

</class>
</hibernate-mapping>

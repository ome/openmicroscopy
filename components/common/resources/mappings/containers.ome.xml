<types>
  
	<type id="ome.model.internal.Link"/>

  <!-- PDI -->
  <type id="ome.model.containers.Project" named="true" described="true">
	  <properties>
		  <child name="datasetLinks" type="ome.model.containers.ProjectDatasetLink"
        target="ome.model.containers.Dataset"/>
	  </properties>
  </type>
  
  <type id="ome.model.containers.Dataset" named="true" described="true">
	  <properties>
		  <parent name="projectLinks" type="ome.model.containers.ProjectDatasetLink"
        target="ome.model.containers.Project"/>
		  <child name="imageLinks" type="ome.model.containers.DatasetImageLink"
        target="ome.model.core.Image"/>
		  <zeromany name="annotations" type="ome.model.annotations.DatasetAnnotation" inverse="dataset"/>
	  </properties>
  </type>
  
  <link id="ome.model.containers.ProjectDatasetLink">
    <properties>
	  	<from_parent type="ome.model.containers.Project"/>
	  	<to_child type="ome.model.containers.Dataset"/>
    </properties>
  </link>
  
  <link id="ome.model.containers.DatasetImageLink">
    <properties>
	  	<from_parent type="ome.model.containers.Dataset"/>
	  	<to_child type="ome.model.core.Image"/>
    </properties>
  </link>

  <!-- CGCI -->
  <type id="ome.model.containers.CategoryGroup" named="true" described="true">
	  <properties>
		  <child name="categoryLinks" type="ome.model.containers.CategoryGroupCategoryLink"
        target="ome.model.containers.Category"/>
	  </properties>
  </type>
  
  <type id="ome.model.containers.Category" named="true" described="true">
	  <properties>
		  <parent name="categoryGroupLinks" type="ome.model.containers.CategoryGroupCategoryLink"
        target="ome.model.containers.CategoryGroup"/>
		  <child name="imageLinks" type="ome.model.containers.CategoryImageLink"
        target="ome.model.core.Image"/>
	  </properties>
  </type>
  
  <link id="ome.model.containers.CategoryGroupCategoryLink">
    <properties>
	  	<from_parent type="ome.model.containers.CategoryGroup"/>
	  	<to_child type="ome.model.containers.Category"/>
    </properties>
  </link>

  <link id="ome.model.containers.CategoryImageLink">
    <properties>
	  	<from_parent type="ome.model.containers.Category"/>
	  	<to_child type="ome.model.core.Image"/>
    </properties>
  </link>

</types>

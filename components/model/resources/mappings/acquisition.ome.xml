<types>
<!-- 
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
		Acquisition Context 1
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 
-->
	<type id="ome.model.core.Image" named="true" described="true" annotated="true"><!-- TODO: review-->
		<properties>
			<!-- Acquisition Context 1 -->
			<required name="acquisitionDate" type="timestamp"/> <!--TODO: optional in the model, Note: in the model this is creationDate but the DB already has a creation date for the object -->
			<optional name="condition" type="ome.model.acquisition.ImagingEnvironment"/><!-- TODO: called ImagingEnvironment in model-->
			<optional name="objectiveSettings" type="ome.model.acquisition.ObjectiveSettings"><!-- TODO: maps to ObjectiveRef (ID and values) in model-->
				<rule id="INS_OBJ_SYNCH" description="only if instrument defined"/>
			</optional>
			<optional name="setup" type="ome.model.acquisition.Instrument"/><!-- TODO: called InstrumentRef (ID only ref) in model-->
			<optional name="position" type="ome.model.acquisition.StageLabel"/><!-- TODO: called StageLabel in model-->
			<optional name="context" type="ome.model.experiment.Experiment"/><!-- TODO: called ExperimentRef (ID only ref) in model-->
			<optional name="defaultPixels" type="ome.model.core.Pixels"/><!-- TODO: required in model-->
			<optional name="acquiredPixels" type="ome.model.core.Pixels"/>
			<zeromany name="pixels" type="ome.model.core.Pixels" inverse="image" ordered="true"/><!-- TODO: onemany in model-->
			<zeromany name="wellSamples" type="ome.model.screen.WellSample" inverse="image"/>
			<!-- Containers -->
			<parent name="datasetLinks" type="ome.model.containers.DatasetImageLink" target="ome.model.containers.Dataset"/>
		</properties>
	</type>
	<type id="ome.model.acquisition.ObjectiveSettings"><!-- TODO: stored in ObjectiveRef in model-->
		<properties>
			<optional name="correctionCollar" type="float"/>
			<optional name="medium" type="ome.model.enums.Medium"/>
			<optional name="refractiveIndex" type="float"/>
			<required name="objective" type="ome.model.acquisition.Objective"/><!-- TODO: ID in model-->
		</properties>
	</type>
	<type id="ome.model.core.Pixels" annotated="true"><!-- TODO: review-->
		<properties>
			<!-- Acquisition Context 1 -->
			<manyone name="image" type="ome.model.core.Image" ordered="true"/><!-- TODO: is manyone correct?-->
			<optional name="relatedTo" type="ome.model.core.Pixels"/><!--TODO: not in model-->
			<required name="pixelsType" type="ome.model.enums.PixelsType"/><!-- TODO: called PixelType in model-->
			<required name="sizeX" type="int"/>
			<required name="sizeY" type="int"/>
			<required name="sizeZ" type="int"/>
			<required name="sizeC" type="int"/>
			<required name="sizeT" type="int"/>
			<required name="sha1" type="string"/>
			<required name="dimensionOrder" type="ome.model.enums.DimensionOrder"/>
			<optional name="physicalSizeX" type="float"/>
			<optional name="physicalSizeY" type="float"/>
			<optional name="physicalSizeZ" type="float"/>
			<optional name="waveStart" type="int"/>
			<optional name="waveIncrement" type="int"/>
			<optional name="timeIncrement" type="float"/>
			<optional name="methodology" type="string"/>
			<onemany name="planeInfo" type="ome.model.core.PlaneInfo" inverse="pixels"><!-- TODO: called Plane in model, zeromany in model-->
				<rule id="PLANE_INFO_CARDINALITY" description="0|sizez*sizey*sizec"/>
			</onemany>
			<!-- TODO: storage -->
			<parent name="pixelsFileMaps" type="ome.model.core.PixelsOriginalFileMap" target="ome.model.core.OriginalFile"/>
			<!-- Acquisition Context 2 -->
			<onemany name="channels" type="ome.model.core.Channel" inverse="pixels" ordered="true">
				<rule id="CHANNEL_CARDINALITY" description="Must be sizeC channels"/>
			</onemany>
			<!--Display Settings 1-->
			<zeromany name="settings" type="ome.model.display.RenderingDef" inverse="pixels"/>
			<zeromany name="thumbnails" type="ome.model.display.Thumbnail" inverse="pixels"/>
		</properties>
	</type>
	<link id="ome.model.core.PixelsOriginalFileMap">
		<properties>
			<from_parent type="ome.model.core.OriginalFile"/>
			<to_child type="ome.model.core.Pixels"/>
		</properties>
	</link>
	<type id="ome.model.core.OriginalFile" named="true" annotated="true">
		<properties>
			<child name="pixelsFileMaps" type="ome.model.core.PixelsOriginalFileMap" target="ome.model.core.Pixels"/>
			<required name="path" type="string"/>
			<required name="size" type="long"/>
			<optional name="atime" type="timestamp"/>
			<optional name="mtime" type="timestamp"/>
			<optional name="ctime" type="timestamp"/>
			<required name="sha1" type="string"/>
			<required name="format" type="ome.model.enums.Format"/>
		</properties>
	</type>
	<type id="ome.model.core.PlaneInfo" annotated="true"><!-- TODO: called Plane in model-->
		<properties>
			<manyone name="pixels" type="ome.model.core.Pixels"/>
			<required name="theZ" type="int"/>
			<required name="theC" type="int"/>
			<required name="theT" type="int"/>
			<optional name="deltaT" type="float"/><!-- TODO: stored in PlaneTiming in model-->
			<optional name="positionX" type="float"/><!-- TODO: stored in StagePosition in model-->
			<optional name="positionY" type="float"/><!-- TODO: stored in StagePosition in model-->
			<optional name="positionZ" type="float"/><!-- TODO: stored in StagePosition in model-->
			<optional name="exposureTime" type="float"/><!-- TODO: stored in PlaneTiming in model-->
			<!-- TODO: see mac notes -->
		</properties>
	</type>
	<type id="ome.model.experiment.Experiment" described="true">
		<properties>
			<required name="type" type="ome.model.enums.ExperimentType"/>
			<zeromany name="microbeamManipulation" type="ome.model.experiment.MicrobeamManipulation" inverse="experiment"/>
		</properties>
	</type>
	<type id="ome.model.experiment.MicrobeamManipulation" described="true">
		<properties>
			<required name="experimenter" type="ome.model.meta.Experimenter"/>
			<required name="type" type="ome.model.enums.MicrobeamManipulationType"/>
			<onemany name="roi" type="ome.model.display.Roi" inverse="microbeamManipulation"/>
			<manyone name="experiment" type="ome.model.experiment.Experiment"/>
		</properties>
	</type>
	<type id="ome.model.acquisition.StageLabel" named="true"><!-- TODO: review-->
		<properties>
			<optional name="positionX" type="float"/><!-- TODO: called X in model-->
			<optional name="positionY" type="float"/><!-- TODO: called Y in model-->
			<optional name="positionZ" type="float"/><!-- TODO: called Z in model-->
		</properties>
	</type>
	<type id="ome.model.acquisition.ImagingEnvironment">
		<properties>
			<optional name="temperature" type="float"/>
			<optional name="airPressure" type="float"/>
			<optional name="humidity" type="float">
				<rule id="HUMIDITY_RANGE" type="range" description="0-1"/>
			</optional>
			<optional name="co2percent" type="float">
				<rule id="CO2_RANGE" type="range" description="0-1"/>
			</optional>
		</properties>
	</type>
<!-- 
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
			Acquisition Context 2
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 
-->
	<type id="ome.model.core.Channel" annotated="true"><!-- TODO: not in model-->
		<properties>
			<optional name="statsInfo" type="ome.model.stats.StatsInfo"><!--TODO: not in model-->
				<rule id="STATSINFO_DEP" type="dependency" description="depends on pixelType"/>
				<!--TODO: depends how?-->
			</optional>
			<optional name="red" type="int">
				<rule id="COLOR_REQ" type="conditionalNull" description="color must exist if PIType == RGB|ARGB|CMYK|HSV"/>
				<rule id="COLOR_RANGE" type="range" description="rule0_255 all values between 0-255"/>
			</optional>
			<optional name="green" type="int">
				<rule id="COLOR_REQ" type="conditionalNull" description="color must exist if PIType == RGB|ARGB|CMYK|HSV"/>
				<rule id="COLOR_RANGE" type="range" description="rule0_255 all values between 0-255"/>
			</optional>
			<optional name="blue" type="int">
				<rule id="COLOR_REQ" type="conditionalNull" description="color must exist if PIType == RGB|ARGB|CMYK|HSV"/>
				<rule id="COLOR_RANGE" type="range" description="rule0_255 all values between 0-255"/>
			</optional>
			<optional name="alpha" type="int">
				<rule id="COLOR_REQ" type="conditionalNull" description="color must exist if PIType == RGB|ARGB|CMYK|HSV"/>
				<rule id="COLOR_RANGE" type="range" description="rule0_255 all values between 0-255"/>
			</optional>
			<optional name="logicalChannel" type="ome.model.core.LogicalChannel"/>
			<manyone name="pixels" type="ome.model.core.Pixels" ordered="true"/>
		</properties>
	</type>
	<type id="ome.model.core.LogicalChannel"><!-- TODO: review-->
		<properties>
			<optional name="name" type="string"/>
			<optional name="pinHoleSize" type="float"/>
			<optional name="illumination" type="ome.model.enums.Illumination"/><!-- TODO: called IlluminationType in model-->
			<optional name="contrastMethod" type="ome.model.enums.ContrastMethod"/>
			<optional name="excitationWave" type="int"/><!-- TODO: called exWave in model-->
			<optional name="emissionWave" type="int"/><!-- TODO: called emWave in model-->
			<optional name="fluor" type="string"/>
			<optional name="ndFilter" type="float"/>
			<optional name="otf" type="ome.model.acquisition.OTF"/><!-- TODO: called OTFRef (ID only Ref) in model-->
			<optional name="detectorSettings" type="ome.model.acquisition.DetectorSettings"><!-- TODO: called DetectorRef (ID and values) in model-->
				<rule id="DETECTOR_SETTINGS_REQ" type="conditionalNull" description="if defined, the instrument must be defined"/>
			</optional>
			<optional name="lightSourceSettings" type="ome.model.acquisition.LightSettings"/><!-- TODO: called LightSourceRef (ID and values) in model-->
			<optional name="filterSet" type="ome.model.acquisition.FilterSet"/><!-- TODO: called FilterSetRef (ID only Ref) in model-->
			<optional name="secondaryEmissionFilter" type="ome.model.acquisition.Filter"/>
			<optional name="secondaryExcitationFilter" type="ome.model.acquisition.Filter"/>
			<optional name="samplesPerPixel" type="int"/>
			<optional name="photometricInterpretation" type="ome.model.enums.PhotometricInterpretation"/>
			<optional name="mode" type="ome.model.enums.AcquisitionMode"/>
			<optional name="pockelCellSetting" type="int"/>
		</properties>
	</type>
	<type id="ome.model.stats.StatsInfo"><!--TODO: not in model-->
		<properties>
			<required name="globalMin" type="double"/><!--TODO: not in model-->
			<required name="globalMax" type="double"/><!--TODO: not in model-->
		</properties>
	</type>
	<type id="ome.model.acquisition.DetectorSettings"><!-- in model as DetectorRef-->
		<properties>
			<optional name="voltage" type="float"/>
			<optional name="gain" type="float"/>
			<optional name="offsetValue" type="float"/><!-- TODO: called Offset in model-->
			<optional name="readOutRate" type="float"/>
			<optional name="binning" type="ome.model.enums.Binning"/>
			<required name="detector" type="ome.model.acquisition.Detector"/><!-- TODO: ID in model-->
		</properties>
	</type>
	<type id="ome.model.acquisition.OTF"><!-- TODO: review-->
		<properties>
			<required name="sizeX" type="int"/>
			<required name="sizeY" type="int"/>
			<required name="opticalAxisAvg" type="boolean"/><!-- TODO: called OpticalAxisAveraged in model-->
			<required name="pixelsType" type="ome.model.enums.PixelsType"/><!-- TODO: called PixelType and using ExtendedPixelTypes in model-->
			<required name="path" type="string"/><!-- TODO: no FK? --><!--TODO: not in model-->
			<!--TODO: BinaryFile in model - is this path?-->
			<optional name="filterSet" type="ome.model.acquisition.FilterSet"/>
			<required name="objective" type="ome.model.acquisition.Objective"/>
			<!-- TODO: rule to check image_link_objective ? -->
			<manyone name="instrument" type="ome.model.acquisition.Instrument"/>
		</properties>
	</type>
	<type id="ome.model.acquisition.LightSettings"><!-- in model as LightSourceRef-->
		<properties>
			<optional name="attenuation" type="float">
				<rule id="ATTENUATION_RANGE" type="range" description="0-1"/>
			</optional>
			<optional name="wavelength" type="int"/>
			<required name="lightSource" type="ome.model.acquisition.LightSource"/><!-- TODO: ID in model-->
		</properties>
	</type>
<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

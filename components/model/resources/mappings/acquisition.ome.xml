<types>

  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    		Acquisition Context 1 
       ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

	<type id="ome.model.core.Image" named="true" described="true" annotated="true"><!--TODO named/desc-->
		<properties>
			<!-- Acquisition Context 1 -->      
			<optional name="condition" type="ome.model.acquisition.ImagingEnvironment"/>
			<optional name="objectiveSettings" type="ome.model.acquisition.ObjectiveSettings">
				<rule id="INS_OBJ_SYNCH" description="only if instrument defined"/>
			</optional>
			<optional name="setup" type="ome.model.acquisition.Instrument"/>
			<optional name="position" type="ome.model.acquisition.StageLabel"/>
			<optional name="context" type="ome.model.experiment.Experiment"/>
      <!-- TODO: name switch and active Pixels isn't really optional! -->      
			<zeromany name="pixels" type="ome.model.core.Pixels" inverse="image" ordered="true"/>

			<!-- Containers -->
			<parent name="categoryLinks" type="ome.model.containers.CategoryImageLink"
				target="ome.model.containers.Category"/>
			<parent name="datasetLinks" type="ome.model.containers.DatasetImageLink"
				target="ome.model.containers.Dataset"/>			
			<child name="annotationLinks"
				type="ome.model.annotations.ImageAnnotationLink"
				target="ome.model.annotations.Annotation"/>

		</properties>
	</type>
	
	<type id="ome.model.acquisition.ObjectiveSettings">
		<properties>
			<optional name="correctionCollar" type="float"/>
			<optional name="medium" type="ome.model.enums.Medium"/>
			<optional name="refractiveIndex" type="float"/>
			<required name="objective" type="ome.model.acquisition.Objective"/>
		</properties>
	</type>

	<type id="ome.model.core.Pixels">
		<properties>
      <!-- Acquisition Context 1 -->      
			<manyone name="image" type="ome.model.core.Image" ordered="true"/>
			<optional name="relatedTo" type="ome.model.core.Pixels"/>
			<!-- TODO: seems insufficient -->
 			<required name="pixelsType" type="ome.model.enums.PixelsType"/>		
			<required name="sizeX" type="int"/>
			<required name="sizeY" type="int"/>
			<required name="sizeZ" type="int"/>
			<required name="sizeC" type="int"/>
			<required name="sizeT" type="int"/>
			<required name="sha1" type="string"/>
			<required name="dimensionOrder" type="ome.model.enums.DimensionOrder"/>
			<optional name="methodology" type="string"/>
 			<onemany name="planeInfo" type="ome.model.core.PlaneInfo" inverse="pixels">
				<rule id="PLANE_INFO_CARDINALITY" description="0|sizez*sizey*sizec"/>
			</onemany>
			<required name="pixelsDimensions" type="ome.model.core.PixelsDimensions"/>
			<!-- TODO: storage -->
			<parent name="pixelsFileMaps" type="ome.model.core.PixelsOriginalFileMap"
        target="ome.model.core.OriginalFile"/>
            
      <!-- Acquisition Context 2 -->            
			<onemany name="channels" type="ome.model.core.Channel" inverse="pixels" ordered="true">
        <rule id="CHANNEL_CARDINALITY" description="Must be sizeC channels"/>
      </onemany>
      
			<!--Display Settings 1-->
			<zeromany name="settings" type="ome.model.display.RenderingDef" inverse="pixels"/>
			<zeromany name="thumbnails" type="ome.model.display.Thumbnail" inverse="pixels"/>
		</properties>
	</type>

	<link id="ome.model.core.PixelsOriginalFileMap">
		<properties>
			<from_parent type="ome.model.core.OriginalFile"/>
			<to_child type="ome.model.core.Pixels"/>
		</properties>
	</link>
	      
	<type id="ome.model.core.OriginalFile">
		<properties>
			<child name="pixelsFileMaps" type="ome.model.core.PixelsOriginalFileMap"
        target="ome.model.core.Pixels"/>
			<required name="path" type="string"/>
			<required name="name" type="string"/>
			<required name="size" type="long"/>
			<optional name="atime" type="timestamp"/>
			<optional name="mtime" type="timestamp"/>
			<optional name="ctime" type="timestamp"/>
			<required name="sha1" type="string"/>
			<required name="format" type="ome.model.enums.Format"/> 
		</properties>
	</type>        
  
  <type id="ome.model.core.PixelsDimensions">
		<properties>
			<required name="sizeX" type="float"/>
			<required name="sizeY" type="float"/>
			<required name="sizeZ" type="float"/>
			<rule id="DEFAULT_MICRONS" type="default" desc="for all default = 1"/>
		</properties>
	</type>
  
	<type id="ome.model.core.PlaneInfo">
		<properties>
			<manyone name="pixels" type="ome.model.core.Pixels"/>
			<required name="theZ" type="int"/>
			<required name="theC" type="int"/>
			<required name="theT" type="int"/>
			<required name="timestamp" type="float"/>
			<optional name="positionX" type="float"/>
			<optional name="positionY" type="float"/>
			<optional name="positionZ" type="float"/>
			<optional name="exposureTime" type="float"/><!-- TODO: see mac notes -->
		</properties>
	</type>
  
	<type id="ome.model.experiment.Experiment" described="true">
		<properties>
			<required name="type" type="ome.model.enums.ExperimentType"/>
		</properties>
	</type>

	<type id="ome.model.acquisition.StageLabel">
		<properties>
      <required name="name" type="string"/>
			<required name="positionX" type="float"/>
			<required name="positionY" type="float"/>
			<required name="positionZ" type="float"/>
		</properties>
	</type>
  
	<type id="ome.model.acquisition.ImagingEnvironment">
		<properties>
			<optional name="temperature" type="float"/>
			<optional name="airPressure" type="float"/>
			<optional name="humidity" type="float">
				<rule id="HUMIDITY_RANGE" type="range" description="0-1"/>
			</optional>
			<optional name="co2percent" type="float">
				<rule id="CO2_RANGE" type="range" description="0-1"/>
			</optional>
		</properties>
	</type>		

  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    		Acquisition Context 2
       ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

	<type id="ome.model.core.Channel" >
		<properties>
			<optional name="statsInfo" type="ome.model.stats.StatsInfo">
        <rule id="STATSINFO_DEP" type="dependency" 
          description="depends on pixelType"/><!--TODO: depends how?-->
      </optional>
			<optional name="colorComponent" type="ome.model.display.Color">
        <rule id="COLOR_REQ" type="conditionalNull"
          description="color must exist if PIType == RGB|ARGB|CMYK|HSV"/>
      </optional>
			<optional name="logicalChannel" type="ome.model.core.LogicalChannel"/>
			<manyone name="pixels" type="ome.model.core.Pixels" ordered="true"/>
		</properties>
	</type>
  
	<type id="ome.model.core.LogicalChannel">
		<properties>
			<optional name="name" type="string"/>
			<optional name="pinHoleSize" type="int"/>
			<optional name="illumination" type="ome.model.enums.Illumination"/>
			<optional name="contrastMethod" type="ome.model.enums.ContrastMethod"/>
			<optional name="excitationWave" type="int"/>
			<optional name="emissionWave" type="int"/>
			<optional name="fluor" type="string"/>
			<optional name="ndFilter" type="float"/>
			<optional name="otf" type="ome.model.acquisition.OTF"/>
			<optional name="detectorSettings" type="ome.model.acquisition.DetectorSettings">
				<rule id="DETECTOR_SETTINGS_REQ" type="conditionalNull"
					description="if defined, the instrument must be defined"/>
			</optional>
			<optional name="lightSource" type="ome.model.acquisition.LightSettings"/>
			<optional name="auxLightSource" type="ome.model.acquisition.LightSettings"/>  
			<optional name="photometricInterpretation" type="ome.model.enums.PhotometricInterpretation"/>
			<optional name="mode" type="ome.model.enums.AcquisitionMode"/>
			<optional name="pockelCellSetting" type="string"/>
		</properties>
	</type>
	
	<type id="ome.model.stats.StatsInfo">
		<properties>
			<required name="globalMin" type="double"/>
			<required name="globalMax" type="double"/>
		</properties>
	</type>

	<type id="ome.model.acquisition.DetectorSettings">
		<properties>
			<optional name="voltage" type="float"/>
			<optional name="gain" type="float"/>
			<optional name="offsetValue" type="float"/>
			<optional name="readOutRate" type="float"/>
			<optional name="binning" type="ome.model.enums.Binning"/>
			<optional name="amplification" type="float"/>
			<required name="detector" type="ome.model.acquisition.Detector"/>
		</properties>
	</type>

	<type id="ome.model.acquisition.OTF">
		<properties>
			<required name="sizeX" type="int"/>
			<required name="sizeY" type="int"/>
			<required name="opticalAxisAvg" type="boolean"/>
			<required name="pixelType" type="ome.model.enums.PixelsType"/>
			<required name="path" type="string"/><!-- TODO: no FK? -->
      <!-- TODO: rule to check image_link_objective ? -->
		</properties>
	</type>

	<type id="ome.model.acquisition.LightSettings">
		<properties>
			<optional name="power" type="float"/>
			<optional name="technique" type="string"/>
			<required name="lightSource" type="ome.model.acquisition.LightSource"/>
		</properties>
	</type>

  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    		Acquisition Context 3
       ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

	<abstract id="ome.model.acquisition.LightSource" manufactured="true">
		<properties>
			<required name="manufacturer" type="string"/>
			<required name="model" type="string"/>
			<required name="power" type="float"/>
			<optional name="serialNumber" type="string"/>
			<manyone name="instrument" type="ome.model.acquisition.Instrument"/>
		</properties>
	</abstract>
  
	<type id="ome.model.acquisition.Arc"
		superclass="ome.model.acquisition.LightSource">
		<properties>
			<required name="type" type="ome.model.enums.ArcType"/>
		</properties>
	</type>
	
	<type id="ome.model.acquisition.Filament"
		superclass="ome.model.acquisition.LightSource">
		<properties>
			<required name="type" type="ome.model.enums.FilamentType"/>
		</properties>
	</type>
	
	<type id="ome.model.acquisition.Laser"
		superclass="ome.model.acquisition.LightSource">
		<properties>
			<required name="type" type="ome.model.enums.LaserType"/>
			<required name="laserMedium" type="ome.model.enums.LaserMedium"/>
			<required name="frequencyMultiplication" type="ome.model.enums.FrequencyMultiplication"/>
			<required name="tunable" type="boolean"/>
			<optional name="pulse" type="ome.model.enums.Pulse"/>
			<optional name="wavelength" type="int"/>
			<required name="pockelCell" type="boolean"/>
			<optional name="pump" type="ome.model.acquisition.Laser"/>
		</properties>
	</type>
  	
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    		Acquisition Context 4
       ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

	<type id="ome.model.acquisition.Detector" manufactured="true">
		<properties>
			<required name="manufacturer" type="string"/>
			<required name="model" type="string"/>
			<optional name="serialNumber" type="string"/>
			<optional name="voltage" type="float"/>
			<optional name="gain" type="float"/>
			<optional name="offsetValue" type="float"/>
			<required name="type" type="ome.model.enums.DetectorType"/>
			<manyone name="instrument" type="ome.model.acquisition.Instrument"/>
		</properties>
	</type>
	  
	<type id="ome.model.acquisition.Microscope" manufactured="true">
		<properties>
			<required name="manufacturer" type="string"/>
			<required name="model" type="string"/>
			<optional name="serialNumber" type="string"/>
			<required name="type" type="ome.model.enums.MicroscopeType"/>
		</properties>
	</type>

	<type id="ome.model.acquisition.Objective" manufactured="true">
		<properties>
			<required name="manufacturer" type="string"/>
			<required name="model" type="string"/>
			<optional name="serialNumber" type="string"/>
			<required name="magnificiation" type="double"/>
			<required name="lensNA" type="float"/>
			<optional name="immersion" type="ome.model.enums.Immersion"/>
			<optional name="coating" type="ome.model.enums.Coating"/>
			<manyone name="instrument" type="ome.model.acquisition.Instrument"/>
		</properties>
	</type>
  
	<type id="ome.model.acquisition.Instrument">
		<properties>
			<required name="microscope" type="ome.model.acquisition.Microscope"/>
			<onemany name="detector" type="ome.model.acquisition.Detector" inverse="instrument"/>
			<onemany name="objective" type="ome.model.acquisition.Objective" inverse="instrument"/>
			<onemany name="lightSource" type="ome.model.acquisition.LightSource" inverse="instrument"/>
			<onemany name="filter" type="ome.model.acquisition.Filter" inverse="instrument"/>
		</properties>
	</type>
	
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    		Acquisition Context 5
       ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  
	<type id="ome.model.acquisition.Filter">
		<properties>
			<required name="customized" type="boolean"/>
			<optional name="filterSet" type="ome.model.acquisition.FilterSet"/>
			<optional name="customizedFilterSet" 
				type="ome.model.acquisition.CustomizedFilterSet"/>
				<rule id="FILTER_XOR" type="xor" 
					escription="either filter set or customized filter set"/>
			<manyone name="instrument" type="ome.model.acquisition.Instrument"/>
		</properties>
	</type>
	
	<type id="ome.model.acquisition.FilterSet" manufactured="true">
		<properties>
			<required name="manufacturer" type="string"/>
			<required name="model" type="string"/>
			<optional name="serialNumber" type="string"/>
			<optional name="transmittanceRange" type="ome.model.acquisition.TransmittanceRange"/>
		</properties>
	</type>      
  
  <type id="ome.model.acquisition.CustomizedFilterSet" manufactured="true">
		<properties>
			<required name="manufacturer" type="string"/>
			<required name="model" type="string"/>
			<optional name="serialNumber" type="string"/>
			<required name="excitationFilter" type="ome.model.acquisition.ExcitationFilter"/>
			<required name="dichroic" type="ome.model.acquisition.Dichroic"/>
			<required name="emissionFilter" type="ome.model.acquisition.EmissionFilter"/>
			<optional name="transmittanceRange" type="ome.model.acquisition.TransmittanceRange"/>
		</properties>
	</type>

	<type id="ome.model.acquisition.Dichroic"/>
  	
	<type id="ome.model.acquisition.ExcitationFilter">
		<properties>
			<required name="type" type="ome.model.enums.FilterType"/>
			<optional name="transmittanceRange" type="ome.model.acquisition.TransmittanceRange"/>
		</properties>
	</type>
	
	<type id="ome.model.acquisition.EmissionFilter">
		<properties>
			<required name="type" type="ome.model.enums.FilterType"/>
			<optional name="transmittanceRange" type="ome.model.acquisition.TransmittanceRange"/>
		</properties>
	</type>
	
	<type id="ome.model.acquisition.TransmittanceRange">
		<properties>
			<required name="cutIn" type="int"/>
			<required name="cutOut" type="int"/>
			<optional name="cutInTolerance" type="int"/>
			<optional name="cutOutTolerance" type="int"/>
			<required name="transmittance" type="float"/>
		</properties>
	</type>
  	

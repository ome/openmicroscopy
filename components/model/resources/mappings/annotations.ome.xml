<types>
<!-- Structured Annotations -->
	<abstract id="ome.model.annotations.Annotation" discriminator="/" annotated="true">
		<properties>
			<optional name="ns" type="string"/>
			<optional name="name" type="string"/>
			<optional name="description" type="text"/>
			<parent name="channelLinks" type="ome.model.annotations.ChannelAnnotationLink" target="ome.model.core.Channel"/>
			<parent name="datasetLinks" type="ome.model.annotations.DatasetAnnotationLink" target="ome.model.containers.Dataset"/>
			<parent name="detectorLinks" type="ome.model.annotations.DetectorAnnotationLink" target="ome.model.acquisition.Detector"/>
			<parent name="dichroicLinks" type="ome.model.annotations.DichroicAnnotationLink" target="ome.model.acquisition.Dichroic"/>
			<parent name="experimenterLinks" type="ome.model.annotations.ExperimenterAnnotationLink" target="ome.model.meta.Experimenter"/>
			<parent name="experimenterGroupLinks" type="ome.model.annotations.ExperimenterGroupAnnotationLink" target="ome.model.meta.ExperimenterGroup"/>
			<parent name="filesetLinks" type="ome.model.annotations.FilesetAnnotationLink" target="ome.model.fs.Fileset"/>
			<parent name="filterLinks" type="ome.model.annotations.FilterAnnotationLink" target="ome.model.acquisition.Filter"/>
			<parent name="folderLinks" type="ome.model.annotations.FolderAnnotationLink" target="ome.model.containers.Folder"/>
			<parent name="imageLinks" type="ome.model.annotations.ImageAnnotationLink" target="ome.model.core.Image"/>
			<parent name="instrumentLinks" type="ome.model.annotations.InstrumentAnnotationLink" target="ome.model.acquisition.Instrument"/>
			<parent name="lightPathLinks" type="ome.model.annotations.LightPathAnnotationLink" target="ome.model.acquisition.LightPath"/>
			<parent name="lightSourceLinks" type="ome.model.annotations.LightSourceAnnotationLink" target="ome.model.acquisition.LightSource"/>
			<parent name="namespaceLinks" type="ome.model.annotations.NamespaceAnnotationLink" target="ome.model.meta.Namespace"/>
			<parent name="nodeLinks" type="ome.model.annotations.NodeAnnotationLink" target="ome.model.meta.Node"/>
			<parent name="objectiveLinks" type="ome.model.annotations.ObjectiveAnnotationLink" target="ome.model.acquisition.Objective"/>
			<parent name="originalFileLinks" type="ome.model.annotations.OriginalFileAnnotationLink" target="ome.model.core.OriginalFile"/>
			<parent name="planeInfoLinks" type="ome.model.annotations.PlaneInfoAnnotationLink" target="ome.model.core.PlaneInfo"/>
			<parent name="plateAcquisitionLinks" type="ome.model.annotations.PlateAcquisitionAnnotationLink" target="ome.model.screen.PlateAcquisition"/>
			<parent name="plateLinks" type="ome.model.annotations.PlateAnnotationLink" target="ome.model.screen.Plate"/>
			<parent name="projectLinks" type="ome.model.annotations.ProjectAnnotationLink" target="ome.model.containers.Project"/>
			<parent name="reagentLinks" type="ome.model.annotations.ReagentAnnotationLink" target="ome.model.screen.Reagent"/>
			<parent name="roiLinks" type="ome.model.annotations.RoiAnnotationLink" target="ome.model.roi.Roi"/>
			<parent name="screenLinks" type="ome.model.annotations.ScreenAnnotationLink" target="ome.model.screen.Screen"/>
			<parent name="sessionLinks" type="ome.model.annotations.SessionAnnotationLink" target="ome.model.meta.Session"/>
			<parent name="shapeLinks" type="ome.model.annotations.ShapeAnnotationLink" target="ome.model.roi.Shape"/>
			<parent name="wellLinks" type="ome.model.annotations.WellAnnotationLink" target="ome.model.screen.Well"/>
		</properties>
	</abstract>
<!-- top-level types -->
	<type id="ome.model.annotations.ListAnnotation" discriminator="/list/" superclass="ome.model.annotations.Annotation"/>
	<abstract id="ome.model.annotations.BasicAnnotation" discriminator="/basic/" superclass="ome.model.annotations.Annotation"/><!-- not used in data model -->
	<abstract id="ome.model.annotations.TypeAnnotation" discriminator="/type/" superclass="ome.model.annotations.Annotation"/><!-- not used in data model -->
<!-- /basic/text -->
	<abstract id="ome.model.annotations.TextAnnotation" discriminator="/basic/text/" superclass="ome.model.annotations.Annotation">
		<properties>
			<optional name="textValue" type="text"/>
		</properties>
	</abstract>
	<type id="ome.model.annotations.CommentAnnotation" discriminator="/basic/text/comment/" superclass="ome.model.annotations.TextAnnotation"/>
	<type id="ome.model.annotations.TagAnnotation" discriminator="/basic/text/tag/" superclass="ome.model.annotations.TextAnnotation"/>
	<type id="ome.model.annotations.XmlAnnotation" discriminator="/basic/text/xml/" superclass="ome.model.annotations.TextAnnotation"/>
<!--  /basic/num -->
	<abstract id="ome.model.annotations.NumericAnnotation" discriminator="/basic/num/" superclass="ome.model.annotations.BasicAnnotation"/>
	<type id="ome.model.annotations.LongAnnotation" discriminator="/basic/num/long/" superclass="ome.model.annotations.NumericAnnotation">
		<properties>
			<optional name="longValue" type="long"/>
		</properties>
	</type>
	<type id="ome.model.annotations.DoubleAnnotation" discriminator="/basic/num/double/" superclass="ome.model.annotations.NumericAnnotation">
		<properties>
			<optional name="doubleValue" type="double"/>
		</properties>
	</type>
<!-- other basic -->
	<type id="ome.model.annotations.BooleanAnnotation" discriminator="/basic/bool/" superclass="ome.model.annotations.BasicAnnotation">
		<properties>
			<optional name="boolValue" type="boolean"/>
		</properties>
	</type>
	<type id="ome.model.annotations.MapAnnotation" discriminator="/map/" superclass="ome.model.annotations.Annotation">
		<properties>
			<map name="mapValue" type="string"/>
		</properties>
	</type>
	<type id="ome.model.annotations.TimestampAnnotation" discriminator="/basic/time/" superclass="ome.model.annotations.BasicAnnotation">
		<properties>
			<optional name="timeValue" type="timestamp"/>
		</properties>
	</type>
	<type id="ome.model.annotations.TermAnnotation" discriminator="/basic/term/" superclass="ome.model.annotations.BasicAnnotation">
		<properties>
			<optional name="termValue" type="text"/>
		</properties>
	</type>
<!--  /type -->
	<type id="ome.model.annotations.FileAnnotation" discriminator="/type/OriginalFile/" superclass="ome.model.annotations.TypeAnnotation">
		<properties>
			<optional name="file" type="ome.model.core.OriginalFile"/>
		</properties>
	</type>
</types>

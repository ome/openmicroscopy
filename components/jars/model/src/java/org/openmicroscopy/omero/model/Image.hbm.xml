<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="org.openmicroscopy.omero.model.Image" 
    table="images"
>

    <id
        name="imageId"
        type="java.lang.Integer"
        column="image_id"
    >
        <generator class="assigned" />
    </id>

    <property
        name="pixels"
        type="org.openmicroscopy.omero.model.ImagePixel"
        column="pixels_id"
        length="4"
    />
    <property
        name="inserted"
        type="timestamp"
        column="inserted"
        not-null="true"
        length="8"
    />
    <property
        name="name"
        type="java.lang.String"
        column="name"
        not-null="true"
        length="256"
    />
    <property
        name="description"
        type="java.lang.String"
        column="description"
        length="-1"
    />
    <property
        name="created"
        type="timestamp"
        column="created"
        not-null="true"
        length="8"
    />
    <property
        name="imageGuid"
        type="java.lang.String"
        column="image_guid"
        length="256"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to Thumbnail -->
    <set
        name="thumbnails"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="image_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.Thumbnail"
        />
    </set>
    <!-- bi-directional one-to-many association to Classification -->
    <set
        name="classifications"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="image_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.Classification"
        />
    </set>
    <!-- bi-directional one-to-many association to DisplayRoi -->
    <set
        name="displayRois"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="image_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.DisplayRoi"
        />
    </set>
    <!-- bi-directional one-to-many association to ImageInfo -->
    <set
        name="imageInfos"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="image_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.ImageInfo"
        />
    </set>
    <!-- bi-directional one-to-many association to ImagePixel -->
    <set
        name="imagePixels"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="image_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.ImagePixel"
        />
    </set>
    <!-- bi-directional one-to-many association to ImagePlate -->
    <set
        name="imagePlates"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="image_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.ImagePlate"
        />
    </set>
    <!-- bi-directional one-to-many association to Feature -->
    <set
        name="features"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="image_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.Feature"
        />
    </set>
    <!-- bi-directional one-to-many association to ImageAnnotation -->
    <set
        name="imageAnnotations"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="image_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.ImageAnnotation"
        />
    </set>
    <!-- bi-directional one-to-many association to ModuleExecution -->
    <set
        name="moduleExecutions"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="image_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.ModuleExecution"
        />
    </set>
    <!-- bi-directional one-to-many association to ImageDimension -->
    <set
        name="imageDimensions"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="image_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.ImageDimension"
        />
    </set>
    <!-- bi-directional one-to-many association to ChannelComponent -->
    <set
        name="channelComponents"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="image_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.ChannelComponent"
        />
    </set>
    <!-- bi-directional one-to-many association to DisplayOption -->
    <set
        name="displayOptions"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="image_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.DisplayOption"
        />
    </set>
    <!-- bi-directional many-to-one association to Group -->
    <many-to-one
        name="group"
        class="org.openmicroscopy.omero.model.Group"
        not-null="true"
    >
        <column name="group_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Experimenter -->
    <many-to-one
        name="experimenter"
        class="org.openmicroscopy.omero.model.Experimenter"
        not-null="true"
    >
        <column name="experimenter_id" />
    </many-to-one>
    <!-- bi-directional many-to-many association to Dataset -->
    <set
        name="datasets"
        lazy="true"
		cascade="all"
        table="image_dataset_map"
    >
        <key>
            <column name="image_id" />
        </key>
        <many-to-many
            class="org.openmicroscopy.omero.model.Dataset"
        >
            <column name="dataset_id" />
        </many-to-many>
    </set>

</class>
</hibernate-mapping>

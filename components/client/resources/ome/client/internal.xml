<!--

  The initial.xml configuration file is used as default system properties
  for all static client contexts. For non-static login, provide an instance
  of java.util.Properties yourself in the OmeroContext.getClientContext call.
  
-->

	<beans>	

  <bean id="properties" class="java.util.Properties">
    <constructor-arg>
      <props/>
    </constructor-arg>
  </bean>
        
</beans>
